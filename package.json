{
  "name": "vscode-typeform",
  "displayName": "Typeform Responses",
  "description": "Get responses for your typeforms in VSCode",
  "version": "0.0.1",
  "publisher": "piyush-bhatt",
  "author": {
    "name": "Piyush Bhatt"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/piyush-bhatt/vscode-typeform"
  },
  "bugs": {
    "url": "https://github.com/piyush-bhatt/vscode-typeform/issues"
  },
  "homepage": "https://github.com/piyush-bhatt/vscode-typeform#readme",
  "license": "SEE LICENSE IN LICENSE",
  "engines": {
    "vscode": "^1.46.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "typeform"
  ],
  "icon": "media/icon/icon.png",
  "activationEvents": [
    "onView:typeform.forms"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "Typeform",
      "properties": {
        "typeform.token": {
          "type": "string",
          "default": "",
          "description": "Typeform Token"
        },
        "typeform.formList": {
          "type": "array",
          "default": [],
          "description": "Array of Typeform forms"
        }
      }
    },
    "commands": [
      {
        "command": "typeform.addToken",
        "title": "Typeform: Add Token",
        "icon": "$(gear)"
      },
      {
        "command": "typeform.addForm",
        "title": "Typeform: Add Form",
        "icon": "$(add)"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "typeform",
          "title": "Typeform",
          "icon": "media/icon/icon.svg"
        }
      ]
    },
    "views": {
      "typeform": [
        {
          "type": "tree",
          "id": "typeform.forms",
          "name": "Forms"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "typeform.forms",
        "contents": "No token added for Typeform.\n[Add Token](command:typeform.addToken)",
        "when": "config.typeform.token == ''"
      },
      {
        "view": "typeform.forms",
        "contents": "Add a form",
        "when": "config.typeform.formList == []"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "typeform.addForm",
          "when": "view == typeform.forms",
          "group": "navigation@0"
        },
        {
          "command": "typeform.addToken",
          "when": "view == typeform.forms",
          "group": "navigation@1"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.46.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.27",
    "eslint": "^7.6.0",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "glob": "^7.1.6",
    "mocha": "^8.0.1",
    "typescript": "^3.8.3",
    "vscode-test": "^1.4.0"
  }
}